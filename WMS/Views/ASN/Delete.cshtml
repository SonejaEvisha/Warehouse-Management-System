@model WMS.tbl_asn

@{
    ViewBag.Title = "Delete";
   
}



<h5 class="text-primary">Delete ASN</h5>
<div>

    <hr />
    <dl class="dl-horizontal">
        <dt class="text-primary">
            Client
        </dt>

        <dd class="text-primary">
            @Model.client_name
        </dd>
        <dt class="text-primary">
            Purchase Order
        </dt>

        <dd class="text-primary">
            @Model.order
          
        </dd>

        <dt class="text-primary">
            Arrival Date
        </dt>

        <dd class="text-primary">
            @Model.arr_date
        </dd>

        <dt class="text-primary">
            Notes
        </dt>

        <dd class="text-primary">
            @Model.notes
        </dd>

     </dl>
    <form action="?SKU1=@Model.asn_id" method="POST" data-toggle="validator" role="form">
        @Html.AntiForgeryToken()

        <div class="form-actions no-color col-md-offset-2">

            <input type="submit" value="Delete" class="btn btn-primary" />

        </div>
    </form>
</div>
@Html.ActionLink("Back to List", "Index")

